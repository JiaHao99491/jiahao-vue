{
  "name": "jiahao-vue",
  "version": "0.3.1",
  "type": "module",
  "description": "Vue Component Library",
  "license": "MIT",
  "author": "JiaHao",
  "homepage": "https://www.jiahao-vue.cn/",
  "repository": "https://github.com/JiaHao99491/jiahao-vue",
  "bugs": {
    "url": "https://github.com/JiaHao99491/jiahao-vue/issues"
  },
  "keywords": [
    "jiahao-vue",
    "tailwind",
    "tailwindcss",
    "tailwind css",
    "tailwind vue",
    "tailwind plugin",
    "tailwind component",
    "tailwind css plugin",
    "css",
    "ui",
    "vue",
    "component",
    "framework",
    "front-end",
    "theme",
    "component library",
    "css library"
  ],
  "files": [
    "jiahaovue"
  ],
  "main": "./jiahaovue/lib.umd.cjs",
  "module": "./jiahaovue/lib.js",
  "types": "./jiahaovue/types/main.d.ts",
  "exports": {
    ".": {
      "types": "./jiahaovue/types/main.d.ts",
      "import": "./jiahaovue/lib.js",
      "require": "./jiahaovue/lib.js"
    },
    "./lib.css": {
      "import": "./jiahaovue/lib.css",
      "require": "./jiahaovue/lib.css"
    },
    "./js": {
      "types": "./jiahaovue/types/main.d.ts",
      "import": "./jiahaovue/lib.js",
      "require": "./jiahaovue/lib.umd.cjs"
    },
    "./globals": {
      "types": "./jiahaovue/types/main.d.ts",
      "import": "./jiahaovue/lib.js",
      "require": "./jiahaovue/lib.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview"
  },
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "@iconify/vue": "^4.3.0",
    "daisyui": "^4.12.24",
    "nanoid": "^5.1.3",
    "radix-vue": "^1.9.17",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@histoire/plugin-vue": "1.0.0-alpha.2",
    "@playwright/test": "^1.51.0",
    "@tailwindcss/vite": "^4.0.13",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.22.0",
    "eslint-plugin-playwright": "^2.2.0",
    "eslint-plugin-vue": "^9.33.0",
    "histoire": "1.0.0-alpha.2",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.3",
    "vite": "^6.2.1",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  }
}
